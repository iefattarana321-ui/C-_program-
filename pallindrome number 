#include <stdio.h>

void main()
{
    int num, n, rev = 0, temp;

    printf("Enter a number to check palindrome: ");
    scanf("%d", &num);

    n = num;   // store original number

    while (num > 0)
    {
        temp = num % 10;
        rev = rev * 10 + temp;
        num = num / 10;
    }

    if (n == rev)
        printf("%d is a palindrome number", n);
    else
        printf("%d is not a palindrome number", n);
}